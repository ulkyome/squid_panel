{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System": "Information"
    }
  },
  "AllowedHosts": "*",
  "SquidSettings": {
    "ConfigPath": "/etc/squid/squid.conf",
    "ConfigDirectory": "/etc/squid/conf.d/",
    "AccessLogPath": "/var/log/squid/access.log",
    "CacheLogPath": "/var/log/squid/cache.log",
    "PidFile": "/var/run/squid.pid"
  },
  "SquidGuardSettings": {
    "ConfigPath": "/etc/squidguard/squidGuard.conf",
    "BlacklistsPath": "/var/lib/squidguard/db/",
    "LogPath": "/var/log/squidguard/squidGuard.log"
  },
  "SystemSettings": {
    "UseSudo": false,
    "ServiceName": "squid",
    "SquidGuardServiceName": "squidguard"
  }
}