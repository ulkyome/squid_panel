{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      },
      "Https": {
        "Url": "https://0.0.0.0:5001"
      }
    }
  },
  "Squid": {
    "ConfigPath": "/etc/squid/squid.conf",
    "ConfigDir": "/etc/squid/conf.d/",
    "AccessLogPath": "/var/log/squid/access.log",
    "CacheLogPath": "/var/log/squid/cache.log",
    "SquidGuardConfigPath": "/etc/squidguard/squidGuard.conf",
    "BlacklistsPath": "/var/lib/squidguard/db/"
  }
}